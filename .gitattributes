# Handle line endings automatically for files detected as text
# and leave all files detected as binary untouched.
*          text=auto
Package*   linguist-detectable=false
xmake.lua  linguist-detectable=false
Makefile   linguist-detectable=false
fixdart.sh linguist-detectable=false
Gemfile    linguist-detectable=false
Rakefile   linguist-detectable=false
*.gemspec  linguist-detectable=false
ruby-fibo  linguist-detectable=false

# c/c++
*.c        text diff=cpp
*.cc       text diff=cpp
*.cxx      text diff=cpp
*.cpp      text diff=cpp
*.cpi      text diff=cpp
*.c++      text diff=cpp
*.hpp      text diff=cpp
*.h        text diff=cpp
*.h++      text diff=cpp
*.hh       text diff=cpp

# c#
*.cs       text diff=csharp
*.cshtml   text diff=html
*.csx      text diff=csharp
*.sln      text eol=crlf
*.csproj   text eol=crlf

# d
*.d        text diff=d

# go
*.go       text diff=go

# f#
*.fs       text diff=fsharp
*.fsx      text diff=fsharp
*.sln      text eol=crlf
*.fsproj   text eol=crlf

# rust
*.rs       text diff=rust
*.toml     text diff=toml
Cargo.lock text

# swift
*.swift    text diff=swift
