    "**/.cargo": true,
    "**/.dart_tool": true,
    "**/.elixir_ls": true,
    "**/.gradle": true,
    "**/.idea": true,
    "**/.mvn": true,
    "**/.nx": true,
    "**/.pants.d": true,
