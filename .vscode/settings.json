    "**/.stack-work": true,
    "**/.turbo": true,
    "**/_build": true,
    "**/alire": true,
    "**/bazel**": true,
    "**/buck-out": true,
    "**/build": true,
    "**/config": true,
    "**/dist": true,
    "**/dist-newstyle": true,
    "**/gradle": true,
    "**/node_modules": true,
    "**/obj": true,
    "**/output": true,
    "**/prelude": true,
    "**/sorbet": true,
    "**/target": true,
    "**/test": true,
    "**/toolchains": true,
    "bin": true
  },
  "java.autobuild.enabled": false,
  "java.compile.nullAnalysis.mode": "automatic",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "basic",
  "terminal.integrated.env.linux": {
    "CARGO_TARGET_DIR": "${workspaceFolder}/dist/rust",
    "DUB_PACKAGE_TARGET_PATH": "${workspaceFolder}/dist/d",
    "DUNE_BUILD_DIR": "${workspaceFolder}/dist/ocaml",
    "NPM_CONFIG_PACK_DESTINATION": "${workspaceFolder}/dist/npm/package"
  },
  "terminal.integrated.env.osx": {
    "CARGO_TARGET_DIR": "${workspaceFolder}/dist/rust",
    "DUB_PACKAGE_TARGET_PATH": "${workspaceFolder}/dist/d",
    "DUNE_BUILD_DIR": "${workspaceFolder}/dist/ocaml",
    "NPM_CONFIG_PACK_DESTINATION": "${workspaceFolder}/dist/npm/package"
  },
  "terminal.integrated.env.windows": {
    "CARGO_TARGET_DIR": "${workspaceFolder}\\dist\\rust",
    "DUB_PACKAGE_TARGET_PATH": "${workspaceFolder}\\dist\\d",
    "DUNE_BUILD_DIR": "${workspaceFolder}\\dist\\ocaml",
    "NPM_CONFIG_PACK_DESTINATION": "${workspaceFolder}\\dist\\npm\\package"
  }
}
